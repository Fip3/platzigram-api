{
  "name": "platzigram-api",
  "version": "1.0.0",
  "description": "Platzigram REST API",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && ava test/**-test.js"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fip3/platzigram-api.git"
  },
  "author": "Felipe Torrejon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fip3/platzigram-api/issues"
  },
  "homepage": "https://github.com/Fip3/platzigram-api#readme",
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-eslint": "^7.1.1",
    "babel-register": "^6.22.0",
    "standard": "^8.6.0",
    "test-listen": "^1.0.1"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "http-hash": "^2.0.0",
    "micro": "^7.0.6",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "uuid-base62": "^0.1.0"
  }
}
